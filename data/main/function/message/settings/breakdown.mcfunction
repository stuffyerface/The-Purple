tellraw @a[tag=ready] ["",{"text":"                                   ","color":"dark_gray","strikethrough":true},"\n","     ",{"translate":"message.purple.modified_setting.title","fallback":"Modified Settings","color":"dark_purple","bold":true},"\n"]

