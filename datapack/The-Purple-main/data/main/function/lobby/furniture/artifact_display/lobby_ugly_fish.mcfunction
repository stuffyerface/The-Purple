# common setup stuff
execute as @n[type=text_display,tag=lobby.artifact_display,tag=empty,distance=..2] run data merge entity @s {Tags:["lobby.artifact_display","full"],view_range:0f}
execute if entity @p[tag=artifact.ugly_fish] run tellraw @a {"translate":"message.purple.lobby.lost_artifact.place","fallback":"%s placed the %s artifact!","with":[{"selector":"@p[tag=artifact.ugly_fish]"},{"translate":"item.purple.artifact.ugly_fish.name","fallback":"Ugly Fish","color":"yellow"}],"color":"#76776F"}
execute unless entity @p[tag=artifact.ugly_fish] run tellraw @a {"translate":"message.purple.lobby.lost_artifact.placed","fallback":"The %s artifact was placed!","with":[{"translate":"item.purple.artifact.ugly_fish.name","fallback":"Ugly Fish","color":"yellow"}],"color":"#76776F"}
tag @s remove empty
tag @s add full
tag @s add artifact.ugly_fish
clear @p[tag=artifact.ugly_fish] *[custom_data={id:"ARTIFACT_UGLY_FISH"}]
tag @p[tag=artifact.ugly_fish] remove artifact.ugly_fish

